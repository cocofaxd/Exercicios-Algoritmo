#include <stdio.h>

int main() {
    int a, b, c;

    // Leitura dos três números
    printf("Digite o primeiro número: ");
    scanf("%d", &a);

    printf("Digite o segundo número: ");
    scanf("%d", &b);

    printf("Digite o terceiro número: ");
    scanf("%d", &c);

    // Impressão dos números na ordem inversa
    printf("Ordem inversa: %d %d %d\n", c, b, a);

    return 0;
}
